{
    "version": "0.2.0",
    "compounds": [
        {
            "name": "DeerFlow Fullstack Dev",
            "configurations": [
                "DeerFlow Backend",
                "DeerFlow Frontend"
            ]
        }
    ],
    "configurations": [
        {
            "name": "Python: 当前文件",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: main.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [
                "--debug",
                "--max_plan_iterations",
                "1",
                "--max_step_num",
                "1"
            ]
        },
        {
            "name": "Python: llm.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/llms/llm.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: server.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/server.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: graph.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/ppt/graph/builder.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "DeerFlow Backend",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/server.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "DeerFlow Frontend",
            "type": "node-terminal",
            "request": "launch",
            "cwd": "${workspaceFolder}/web",
            "command": "/Users/sss/.nvm/versions/node/v22.14.0/bin/pnpm dev",
            "serverReadyAction": {
                "pattern": "- Local:.*(https?://localhost:3000)",
                "uriFormat": "%s",
                "action": "debugWithChrome"
            }
        },
        {
            "name": "DeerFlow Frontend Win",
            "type": "node-terminal",
            "request": "launch",
            "cwd": "${workspaceFolder}/web",
            "command": "pnpm dev",
            "serverReadyAction": {
                "pattern": "- Local:.*(https?://localhost:3000)",
                "uriFormat": "%s",
                "action": "debugWithEdge"
            }
        }
    ]
}